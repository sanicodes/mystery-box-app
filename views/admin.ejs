
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles.css">
    <title>Admin - Mystery Box Results</title>
</head>
<body>
    <h1>Admin Panel - Mystery Box Results</h1>
    <p><a href="/admin-logout">Logout</a></p>
    <table>
        <tr>
            <th>Username</th>
            <th>Result</th>
            <th>Action</th>
        </tr>
        <% data.forEach((entry) => { %>
            <tr>
                <td><%= entry.username %></td>
                <td><%= entry.outcome %></td>
                <td>
                    <form action="/admin/delete/<%= entry.username %>" method="POST" style="display:inline;">
                        <button type="submit" onclick="return confirm('Are you sure you want to delete this entry?');">Delete</button>
                    </form>
                </td>
            </tr>
        <% }); %>
    </table>
</body>
</html>

